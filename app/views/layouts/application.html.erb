<!DOCTYPE html>
<html>
<head>
  <title>ExpenseManager</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= flash[:notice] if flash[:notice].nil?%>
<% if (current_page?(login_new_path)|| current_page?(root_path))%>
    <%= render :partial => 'login/login_layout' %>
<% else %>
    <%= render :partial => 'home/header' %>
<% end %>
<%= yield %>

</body>
</html>
